1. PSEUDOCODE:
START
    Input size
    Initialize array of size 'size'
    FOR i = 0 to size-1
        Input array[i]
    END FOR

    Input target

    FOR i = 0 to size-1
        FOR j = i+1 to size-1
            IF array[i] + array[j] == target THEN
                PRINT i, j
            END IF
        END FOR
    END FOR
END



2.PSEUDOCODE:
START
    Input size
    Initialize array of size 'size'
    FOR i = 0 to size-1
        Input array[i]
    END FOR

    Input target

    Set low = 0
    Set high = size - 1
    Set found = false

    WHILE low <= high DO
        mid = (low + high) / 2

        IF array[mid] == target THEN
            PRINT "Found at index", mid
            Set found = true
            BREAK
        ELSE IF array[mid] < target THEN
            Set low = mid + 1
        ELSE
            Set high = mid - 1
        END IF
    END WHILE

    IF found == false THEN
        PRINT "Not found"
    END IF
END


3.PSEUDOCODE:
START
    Input string1
    Input string2

    IF length of string1 â‰  length of string2 THEN
        PRINT "Not anagram"
    ELSE
        Sort string1
        Sort string2

        IF string1 == string2 THEN
            PRINT "Anagram"
        ELSE
            PRINT "Not anagram"
        END IF
    END IF
END


4.PSEUDOCODE:
START
    Input string
    Set maxLen = 0
    length = length of string

    FOR i = 0 to length - 1
        FOR j = i to length - 1
            IF substring from i to j has all unique characters THEN
                currentLen = j - i + 1
                IF currentLen > maxLen THEN
                    maxLen = currentLen
                END IF
            END IF
        END FOR
    END FOR

    PRINT maxLen
END



5.PSEUDOCODE:
START
    Input string
    Set count = 0
    length = length of string

    FOR i = 0 to length - 1
        FOR j = i to length - 1
            IF substring from i to j is a palindrome THEN
                Increment count
            END IF
        END FOR
    END FOR

    PRINT count
END
